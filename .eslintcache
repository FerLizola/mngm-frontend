  }\r\n\r\n  const cartItemAddHandler = item => {\r\n    cartCtx.addItem({...item, amount: 1});\r\n  };\r\n\r\n  const cartItems = (\r\n    <ul className={styles[\"cart-item\"]}>\r\n      {cartCtx.items.map((item) => {\r\n        return  <CartItem key={item.id} name={item.name} amount={item.amount} price={item.price} \r\n        onRemove={cartItemRemoveHandler.bind(null, item.id)} onAdd={cartItemAddHandler.bind(null, item)}/>;\r\n      })}\r\n    </ul>\r\n  );\r\n  return (\r\n    <Modal onClick={props.onClose}>\r\n      {cartItems}\r\n      <div className={styles.total}>\r\n        <span>Total Amount</span>\r\n        <span>{totalAmount}</span>\r\n      </div>\r\n      <div className={styles.actions}>\r\n          <button className={styles['button--alt']} onClick={props.onClose}>Close</button>\r\n          {hasItem && <button className={styles.button} onClick={createOrder}>Order</button>}\r\n      </div>\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n","C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\store\\cart-context.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\Layout\\HeaderCartButton.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\Cart\\CartItem.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\UI\\Modal.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\Cart\\CartIcon.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\UI\\Card.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\UI\\Input.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\Products\\MainPage.js",[],"C:\\Users\\1000072844\\Documents\\OrderFoodPracticeProject\\src\\components\\Products\\Products.js",["108","109"],"import React from \"react\";\r\nimport styles from \"./Products.module.css\";\r\nimport Card from \"../UI/Card\